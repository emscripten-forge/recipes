+void
+ssconv(int n, float *ritz, float *bounds, float tol, int* nconv)
+{
+    const float eps23 = powf(ulp, 2.0f / 3.0f);
+    float temp = 0.0f;
+
+    *nconv = 0;
+    // Convergence test
+    for (int i = 0; i < n; i++)
+    {
+        temp = fmaxf(eps23, fabsf(ritz[i]));
+        if (fabsf(bounds[i]) <= tol * temp) { *nconv += 1; }
+    }
+
+    return;
+}
