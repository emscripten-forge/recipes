+void
+dsortr(const enum ARPACK_which w, const int apply, const int n, double* x1, double* x2)
+{
+    int i, igap, j;
+    double temp;
+    ARPACK_compare_rfunc *f;
+
+    switch (w)
+    {
+        case which_LM:
+            f = sortr_LM;
+            break;
+        case which_SM:
+            f = sortr_SM;
+            break;
+        case which_LA:
+            f = sortr_LA;
+            break;
+        case which_SA:
+            f = sortr_SA;
+            break;
+        default:
+            f = sortr_LM;
+            break;
+    }
+
+    igap = n / 2;
+
+    while (igap != 0)
+    {
+        j = 0;
+        for (i = igap; i < n; i++)
+        {
+            while (f(x1[j], x1[j+igap]))
+            {
+                if (j < 0) { break; }
+                temp = x1[j];
+                x1[j] = x1[j+igap];
+                x1[j+igap] = temp;
+
+                if (apply)
+                {
+                    temp = x2[j];
+                    x2[j] = x2[j+igap];
+                    x2[j+igap] = temp;
+                }
+                j -= igap;
+            }
+            j = i - igap + 1;
+        }
+        igap = igap / 2;
+    }
+}
