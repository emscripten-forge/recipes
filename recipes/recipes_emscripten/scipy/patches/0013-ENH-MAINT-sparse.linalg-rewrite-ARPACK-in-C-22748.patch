+void
+snapps(int n, int* kev, int np, float* shiftr, float* shifti, float* v,
+       int ldv, float* h, int ldh, float* resid, float* q, int ldq, float* workl,
+       float* workd)
+{
+    int cconj;
+    int i, ir, j, jj, int1 = 1, istart, iend = 0, nr, tmp_int;
+    int kplusp = *kev + np;
+    float smlnum = unfl * ( n / ulp);
