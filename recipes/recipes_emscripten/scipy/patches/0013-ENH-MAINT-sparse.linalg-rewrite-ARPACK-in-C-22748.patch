+    //   Move v(:,kplusp-kev+1:kplusp) into v(:,1:kev).
+
+    for (i = 0; i < *kev; i++)
+    {
+        dcopy_(&n, &v[(kplusp-*kev+i)*ldv], &int1, &v[i*ldv], &int1);
+    }
