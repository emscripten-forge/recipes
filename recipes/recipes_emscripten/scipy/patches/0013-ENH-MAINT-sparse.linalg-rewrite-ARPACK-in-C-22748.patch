+    //  Compute column 1 to kev of (V*Q) in backward order
+    //  taking advantage of the upper Hessenberg structure of Q.
+
+    for (i = 0; i < *kev; i++)
+    {
+        tmp_int = kplusp - i;
+        dgemv_("N", &n, &tmp_int, &dbl1, v, &ldv, &q[(*kev-i-1)*ldq], &int1, &dbl0, workd, &int1);
