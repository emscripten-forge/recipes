+            f = sortr_LA;
+            break;
+        case which_SA:
+            f = sortr_SA;
+            break;
+        default:
+            f = sortr_LM;
+            break;
+    }
+
+    igap = n / 2;
+
+    while (igap != 0)
+    {
+        j = 0;
+        for (i = igap; i < n; i++)
+        {
+            while (f(x[j], x[j + igap]))
+            {
+                if (j < 0) { break; }
