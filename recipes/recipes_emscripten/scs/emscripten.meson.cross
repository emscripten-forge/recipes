# binaries section is at the end as may want to append python binary.

[properties]
needs_exe_wrapper = true
skip_sanity_check = true

[host_machine]
system = 'emscripten'
cpu_family = 'wasm32'
cpu = 'wasm'
endian = 'little'

[binaries]
exe_wrapper = 'node'
pkgconfig = 'pkg-config'
python = '@(PYTHON)'

